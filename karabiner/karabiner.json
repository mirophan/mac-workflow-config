                "from": {
                  "key_code": "f",
                  "modifiers": {
                    "mandatory": [
                      "option"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "right_arrow",
                    "modifiers": [
                      "option"
                    ]
                  }
                ],
                "type": "basic"
              },
              { 
                "conditions": [
                  {
                    "bundle_identifiers": [
                      "^org\\.gnu\\.Emacs",
                      "^com\\.googlecode\\.iterm2"
                    ],
                    "type": "frontmost_application_unless"
                  }
                ],
                "from": {
                  "key_code": "b",
                  "modifiers": {
                    "mandatory": [
                      "option"
                    ]
                  }
                },
                "to": [
                  {
                    "key_code": "left_arrow",
                    "modifiers": [
                      "option"
                    ]
                  }
                ],
                "type": "basic"
              }
            ]
          }
        ]
      },
